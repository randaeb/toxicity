        <div class="post__body">
          <div class="post__header">
            <div class="post__headerText">
              <h1>
            {{username}}
                <span class="post__headerSpecial"
                  ><span class="material-icons post__badge"> TOXINBOARD </span>{{username}}</span
                >
              </h1>
            </div>
            
          </div>
          <img
            src="./assets/images/image.jpg"
            alt=""
          />
       
          <div class="posts ast-group">
  {{#each posts}}
  <div class="post__headerDescription">
              <p>You said it, you MEAN IT!!</p>
            </div>
    <a class="ast-item flex-row justify-space-between" href="/post/{{id}}">
      <span>{{title}}</span>
      <span>{{format_date createdAt}}</span>
    </a>
  {{/each}}
   {{#if post.Comments}}
   <div class="post">
  <article class="card">
    <div class="card-header flex-row justify-space-between">
      <h2>{{post.title}}</h2>
      <div>
        Posted by {{post.User.username}} on {{format_date post.createdAt}}
      </div>
    </div>
    <div class="card-body">
      {{post.body}}
    </div>
  </article>
    <section class="card">
      <div class="card-header">
        <h3>Comments</h3>
      </div>

      <div class="card-body">
        {{#each post.Comments}}
          <div>
            <p>
              {{body}}
            </p>
            <p>
              &mdash; {{User.username}}, {{format_date createdAt}}
            </p>
          </div>
        {{/each}}
      </div>
    </section>
  {{/if}}
   
</div>

</div>

         
        </div>
